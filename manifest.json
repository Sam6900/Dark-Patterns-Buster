{
    "manifest_version": 3,
    "name": "Dark Pattern Detector",
    "version": "1.0",
    "description": "A Chrome extension to detect dark patterns on websites.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "client/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  